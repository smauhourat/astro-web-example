---
const response = await fetch("https://jsonplaceholder.typicode.com/posts");
const posts = await response.json();
console.log(posts);
---

<ul>
  {
    posts.map((post) => (
      <li>
        <h3 class='title'>{post.title}</h3>
        <p>{post.body}</p>
      </li>
    ))
  }
</ul>

<style>
  .title {
    color: coral;
  }
</style>
